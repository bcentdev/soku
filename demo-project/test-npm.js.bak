export function doubled(arr) {
    // return _.map(arr, x => x * 2);
    return arr.map(x => x * 2); // Temporary without lodash
}

export function saveData(data) {
    // return _.omit(data, ['internal', 'debug']);
    // Using object spread causes parse error - using alternative approach
    const result = {};
    for (const key in data) {
        if (key !== 'internal' && key !== 'debug') {
            result[key] = data[key];
        }
    }
    return result; // Temporary without lodash
}

// Test some other lodash functions
export function chunkedData(arr, size) {
    // return _.chunk(arr, size);
    const chunks = [];
    for (let i = 0; i < arr.length; i += size) {
        chunks.push(arr.slice(i, i + size));
    }
    return chunks; // Temporary without lodash
}

